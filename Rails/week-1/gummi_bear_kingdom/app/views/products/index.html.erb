<p id="notice"><%= notice %></p>

<h1>Products</h1>
<div class="products">
  <% @products.each do |product| %>
    <%= link_to product_path(product) do %>
          <img src="/uploads/<%= product.image %>" alt="" style="max-width: 125px; max-height: 125px">
    <% end %>
  <% end %>
</div>
<%= link_to 'New Product', new_product_path %>
